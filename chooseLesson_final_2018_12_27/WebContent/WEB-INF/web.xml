<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>chooseLesson</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>tu.web.Login</servlet-class>
    <init-param>
      <param-name>character</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </servlet>
  <servlet>
  	<servlet-name>Manager</servlet-name>
  	<servlet-class>tu.web.Manager</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ViewCourse</servlet-name>
  	<servlet-class>tu.web.ViewCourseController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddCourse</servlet-name>
  	<servlet-class>tu.web.AddCourseController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CourseList</servlet-name>
  	<servlet-class>tu.web.CourseListController</servlet-class>
  </servlet>
</web-app>